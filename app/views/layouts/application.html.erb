<!DOCTYPE html>
<html>
  <head>
    <title>Mappura</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

<script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>

    <%= javascript_pack_tag 'application' %>
    <script type="text/javascript" charset="utf-8" src="https://map.yahooapis.jp/js/V1/jsapi?appid=<%=ENV['YAHOO_JAPAN_ID']%>"></script>
    <script src="https://unpkg.com/yahoo-map-cluster@latest/bundle/ymap-cluster.js"></script>
    <script src="https://www.gstatic.com/charts/loader.js"></script>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>

  <header class="clearfix">
  </header>

  <div id="center_content">

    <% if @view_form_type == 0 %>
      <div class="side_bar">
        <%= render 'layouts/shared/sidebar' %>
      </div>
    <% end %>

    <div class="main_form">
      <%= yield %>
    </div>

  </div>

  <footer>
    tttt
  </footer>

  </body>
</html>
